{
	"name": "nachi-robot-language-support",
	"displayName": "nachi robot language support",
	"description": "nachi-robot-language-support",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:nachi-robot-language-support.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "sample conf",
			"type": "object",
			"properties": {
				"conf.strItem": {
					"type": "string",
					"default": "127.0.0.1",
					"description": "sample of string"
				},
				"conf.numItem": {
					"type": "number",
					"default": 12,
					"description": "sample of number"
				},
				"conf.objItem": {
					"type": "object",
					"default": {
						"valNum": 0,
						"valStr": "string"
					},
					"description": "sample of object"
				}
			}

		},
		"commands": [
			{
				"command": "nachi-robot-language-support.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	}
}
